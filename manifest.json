{
  "name": "Reddit-Redirect",
  "description": "Short cut to reddit",
  "version": "2.0",
  "permissions": [
    "notifications",
	"contextMenus",
	"tabs",
	"contentSettings",
	"http://*/*",
	"https://*/*"
  ],
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Go to reddit!"
  },
  "manifest_version": 2
}
