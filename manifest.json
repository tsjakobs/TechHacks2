{
  "name": "Google-Redirect",
  "description": "Make the current page red",
  "version": "2.0",
  "permissions": [
    "notifications",
	"contextMenus",
	"tabs",
	"contentSettings",
	"http://*/*",
	"https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page red"
  },
  "manifest_version": 2
}
